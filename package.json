{
  "name": "cssproject4",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass-p4": "node-sass sass/main.scss project4.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass-p4 ",
    "compile:sass-p4": "node-sass sass/main.scss project4.com.css",
    "concat:css-p4": "concat -o styles-concat.css  icon-font.css project4.com.css",
    "prefixer:css-4": "postcss --use autoprefixer -b 'last 10 versions' styles-concat.css -o style.prefix.css",
    "compress:css": "node-sass style.prefix.css styles.css --output-style compressed",
    "build:css": "npm-run-all compile:sass-p4 concat:css-p4 prefixer:css-4 compress:css"
  },
  "author": "rohit",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.9",
    "postcss-cli": "^8.3.1"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
